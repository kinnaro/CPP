class Solution {
public:
    int romanToInt(string s) {
        map<char, int> mp;
        mp['I'] = 1;
        mp['V'] = 5;
        mp['X'] = 10;
        mp['L'] = 50;
        mp['C'] = 100;
        mp['D'] = 500;
        mp['M'] = 1000;
        
        int len = s.size();
        int sum = 0;
        vector<int> nums;
        for(int i=0;i<len;i++){
            if(mp[s[i]] < mp[s[i+1]])
                sum -= mp[s[i]];
            else
                sum += mp[s[i]];
        }
           
        return sum;
    }
};
